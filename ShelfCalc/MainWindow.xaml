<Window x:Class="ShelfCalc.MainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:ShelfCalc"
             mc:Ignorable="d" 
            Title="MainWindow" Height="600" Width="1000" Loaded="Window_Loaded"
            Background="Black">
    <Grid x:Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Name="VisualGrid" Grid.Row="0" Grid.Column="1" >
            <Grid.RowDefinitions>
                <RowDefinition Height="7*"/>
                <RowDefinition Height="2*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <!-- Здесь управляем шириной второй области для слайдов-->
                <ColumnDefinition Width="0*"/>
            </Grid.ColumnDefinitions>
            <!-- <Image Source="StellPicture.bmp" Grid.Row="0" Grid.ColumnSpan="2"/> -->
            <!-- Изображение стеллажа -->
            <Grid Name ="StellarView" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="5*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="8*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="5*"/>
                </Grid.ColumnDefinitions>
                <Rectangle Grid.Column="1" Grid.Row="1" Grid.RowSpan="11" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="3" Grid.Row="1" Grid.RowSpan="11" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="2" Grid.Row="1" Grid.RowSpan="1" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="2" Grid.Row="3" Grid.RowSpan="1" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="2" Grid.Row="5" Grid.RowSpan="1" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="2" Grid.Row="7" Grid.RowSpan="1" Stroke="Lavender" Fill="Gray"/>
                <Rectangle Grid.Column="2" Grid.Row="9" Grid.RowSpan="1" Stroke="Lavender" Fill="Gray"/>
                <TextBlock Grid.Column="4" Grid.Row="10" Text="А" FontSize="30" Foreground="Yellow" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Grid.Column="2" Grid.Row="6" Text="B" FontSize="30" Foreground="Yellow" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Grid.Column="0" Grid.Row="4" Text="D" FontSize="30" Foreground="Yellow" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Grid.Column="2" Grid.Row="0" Text="C" FontSize="30" Foreground="Yellow" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Grid>
            <!-- Изображение стеллажа -->
            <Grid Grid.Row="1" Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*"/>
                    <ColumnDefinition Width="5*"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Foreground="AliceBlue" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap">Длина полки</TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="0" Foreground="AliceBlue" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap">Глубина полки</TextBlock>
                <TextBox Name ="ShelfLength" Grid.Row="0" Grid.Column="1" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="1000"/>
                <TextBox Name ="ShelfWidth" Grid.Row="1" Grid.Column="1" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="300"/>
            </Grid>
                        
            <StackPanel VerticalAlignment="Center" Orientation="Horizontal" Grid.Row="2" Grid.Column="0">
                <RadioButton Name="Side1" Foreground="LemonChiffon">1-сторонний</RadioButton>
                <Rectangle Width="12"/>
                <RadioButton Name="Side2" Foreground="LemonChiffon" IsChecked="True">2-сторонний</RadioButton>
            </StackPanel>
            <Button FontSize="15" Grid.Row="3" Click="Button_Click" Grid.Column="0">Вставить в чертеж 25 mm</Button>
            <Button FontSize="15" Grid.Row="4" Click="Button_Click_1" Grid.Column="0">Вставить в чертеж 12,5 mm</Button>

            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*"/>
                    <ColumnDefinition Width="5*"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Foreground="AliceBlue" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap">Длина полки</TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="0" Foreground="AliceBlue" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap">Глубина полки</TextBlock>
                <TextBox Name ="ShelfLengthSlide" Grid.Row="0" Grid.Column="1" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="1200"/>
                <TextBox Name ="ShelfWidthSlide" Grid.Row="1" Grid.Column="1" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="800"/>
            </Grid>
            <Button FontSize="15" Grid.Row="3" Grid.Column="1" Click="Button_Click_2">Вставить в чертеж</Button>


        </Grid>



        <TabControl Name="MainTab" Grid.Column="0" Background="Black" BorderThickness="0" BorderBrush="Lavender" SelectionChanged="MainTab_SelectionChanged" >

            <!-- Вкладка со стационарными стеллажами -->
            <TabItem Name="Stati" >
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Ellipse Height="10" Width="10" Fill="Black" />
                        <TextBlock  FontSize="20" Margin="3">Стационарные</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <Grid x:Name="GridStacion" ShowGridLines="False">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <!-- Заполнение Квадаратов и заливка  -->
                        <Rectangle Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="4" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="4" Grid.RowSpan="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="6" Grid.RowSpan="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>


                        <TextBlock Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon" FontSize="25">A</TextBlock>
                        <TextBlock Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon" FontSize="25">B</TextBlock>
                        <TextBlock Grid.Row="4" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon" FontSize="25">C</TextBlock>
                        <TextBlock Grid.Row="5" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon" FontSize="25">D</TextBlock>
                        <TextBlock Grid.Row="6" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon" FontSize="25">E</TextBlock>

                        <!--
                        <Rectangle Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2" Fill="DarkGray"  Stroke="Black"></Rectangle>
                        -->

                        <TextBlock Grid.Row="0" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Center" Grid.ColumnSpan="2" Foreground="Goldenrod" >Наименование параметра</TextBlock>

                        <TextBlock Grid.Row="0" Grid.Column="3" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" TextAlignment="Center" Foreground="Goldenrod" >25 мм</TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="5" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" TextAlignment="Center" Foreground="Goldenrod" >12,5 мм</TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="1" FontSize="18" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender">Расстояние от пола до рабочей плоскости нижней полки</TextBlock>
                        <TextBlock Grid.Row="2" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender">Расстояние между полками</TextBlock>
                        <TextBlock Grid.Row="3" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender">Количество полок</TextBlock>
                        <TextBlock Grid.Row="4" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender">Высота верхней полки/крышки</TextBlock>
                        <TextBlock Grid.Row="5" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender" >Высота стойки</TextBlock>
                        <TextBlock Grid.Row="6" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Lavender">Общая высота стеллажа</TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="4" FontSize="18" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Goldenrod" >Положение полок 25 мм</TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="6" FontSize="18" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Left" Foreground="Goldenrod" >Положение полок 12,5 мм</TextBlock>
                        <!-- Область ввода -->
                        <Rectangle Fill="LightGray" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="2" Grid.Row="1"/>
                        <Rectangle Fill="LightGray" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="2" Grid.Row="2"/>
                        <Rectangle Fill="LightGray" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="2" Grid.Row="3"/>
                        <TextBox x:Name="ShelfLowerStat" Grid.Row="1" Grid.Column="2"   FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="42" TextChanged="ShelfLowerStat_TextChanged" />
                        <TextBox x:Name="ShelfDistanceStat" Grid.Row="2" Grid.Column="2"   FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="325" TextChanged="ShelfDistanceStat_TextChanged"/>
                        <TextBox x:Name="ShelfAmountStat" Grid.Row="3" Grid.Column="2"   FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" BorderBrush="LightGray" VerticalAlignment="Center" Background="LightGray" FontWeight="Bold" Text="5" TextChanged="ShelfAmountStat_TextChanged"/>
                        <!-- Область вывода данных -->
                        <TextBlock x:Name="ShelfLowerStat25" Grid.Row="1" Grid.Column="3"  FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="42" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfLowerStat125" Grid.Row="1" Grid.Column="5"  FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="42" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfDistanceStat25" Grid.Row="2" Grid.Column="3" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="345" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfDistanceStat125" Grid.Row="2" Grid.Column="5" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfAmountStat25" Grid.Row="3" Grid.Column="3" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfAmountStat125" Grid.Row="3" Grid.Column="5" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfUpperHeight25" Grid.Row="4" Grid.Column="3" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfUpperHeight125" Grid.Row="4" Grid.Column="5" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfStandHeight25" Grid.Row="5" Grid.Column="3" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfStandHeight125" Grid.Row="5" Grid.Column="5" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfHeight25" Grid.Row="6" Grid.Column="3" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <TextBlock x:Name="ShelfHeight125" Grid.Row="6" Grid.Column="5" FontSize="25" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="5" Margin="1" Foreground="Lavender"/>
                        <!-- Область вывода высот полок -->
                        <StackPanel x:Name="ShelfPosition25" Grid.Column="4" Grid.Row="1" Grid.RowSpan="6" Orientation="Vertical" ></StackPanel>
                        <StackPanel x:Name="ShelfPosition125" Grid.Column="6" Grid.Row="1" Grid.RowSpan="6" Orientation="Vertical"></StackPanel>
                    </Grid>
                </TabItem.Content>
            </TabItem>

            <!--   Вкладка со стеллажами серии Л    -->
            <TabItem x:Name="TabL">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Height="10" Width="10" Fill="Black" />
                        <TextBlock  FontSize="20" Margin="3">Серия Л</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <Grid x:Name="GridSeriesL" ShowGridLines="False">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.5*"/>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <!-- Заполнение Квадаратов и заливка  -->
                        <Rectangle Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="4" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="0" Grid.Column="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="1" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="0" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="4" Grid.RowSpan="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="6" Grid.RowSpan="6" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>

                        <Rectangle Grid.Row="1" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="1" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="2" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="3" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="4" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="5" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="3" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>
                        <Rectangle Grid.Row="6" Grid.Column="5" Fill="Black" Stroke="Lavender" StrokeThickness="1"/>


                        <TextBlock Grid.Row="1"  FontSize="25" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon">A</TextBlock>
                        <TextBlock Grid.Row="2" FontSize="25" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon">B</TextBlock>
                        <TextBlock Grid.Row="4" FontSize="25" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon">C</TextBlock>
                        <TextBlock Grid.Row="5" FontSize="25" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon">D</TextBlock>
                        <TextBlock Grid.Row="6" FontSize="25" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap" Foreground="LemonChiffon">E</TextBlock>

                        <!--
                        <Rectangle Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2" Fill="DarkGray"  Stroke="Black"></Rectangle>
                        -->
                        <TextBlock Grid.Row="0" Grid.Column="1" FontSize="20" Margin="2" VerticalAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Center" Grid.ColumnSpan="2" Foreground="Goldenrod" >Наименование параметра</TextBlock>

